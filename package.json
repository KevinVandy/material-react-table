{
  "version": "0.1.0",
  "license": "MIT",
  "name": "react-table-mui",
  "description": "A fully featured Material-UI implementation of react-table, inspired by material-table, written from the ground up in TypeScript.",
  "author": "Kevin Vandy",
  "keywords": [
    "react-table",
    "material-ui",
    "material-table"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/KevinVandy/react-table-mui"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "homepage": "https://github.com/KevinVandy/react-table-mui#readme",
  "bugs": {
    "url": "https://github.com/KevinVandy/react-table-mui/issues"
  },
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --passWithNoTests",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "module": "dist/react-table-mui.esm.js",
  "size-limit": [
    {
      "path": "dist/react-table-mui.cjs.production.min.js",
      "limit": "20 KB"
    },
    {
      "path": "dist/react-table-mui.esm.js",
      "limit": "20 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@faker-js/faker": "^6.0.0-alpha.2",
    "@mui/icons-material": "^5.2.5",
    "@mui/material": "^5.2.8",
    "@size-limit/preset-small-lib": "^7.0.5",
    "@storybook/addon-a11y": "^6.4.13",
    "@storybook/addon-essentials": "^6.4.13",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.4.13",
    "@storybook/addons": "^6.4.13",
    "@storybook/react": "^6.4.13",
    "@types/faker": "^5.5.9",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@types/react-table": "^7.7.9",
    "babel-loader": "^8.2.3",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-table": "^7.7.0",
    "size-limit": "^7.0.5",
    "storybook-addon-material-ui": "^0.9.0-alpha.24",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@emotion/react": ">=11",
    "@emotion/styled": ">=11",
    "@mui/icons-material": ">=5",
    "@mui/material": ">=5",
    "react": ">=17",
    "react-table": ">=7"
  }
}
